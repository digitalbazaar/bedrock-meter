{
  "name": "bedrock-meter-test",
  "version": "0.0.1-0",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test c8 --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test c8 --reporter=lcovonly npm test",
    "coverage-report": "c8 report"
  },
  "dependencies": {
    "base58-universal": "^1.0.0",
    "@bedrock/core": "^5.0.0",
    "@bedrock/did-context": "^2.0.0",
    "@bedrock/express": "^4.0.0",
    "@bedrock/ledger-context": "^16.0.0",
    "@bedrock/meter": "file://..",
    "@bedrock/mongodb": "^9.0.0",
    "@bedrock/package-manager": "^2.0.0",
    "@bedrock/security-context": "^4.0.0",
    "@bedrock/server": "^4.0.0",
    "@bedrock/test": "^7.0.0",
    "@bedrock/validation": "^6.0.0",
    "bnid": "^2.0.0",
    "c8": "^7.11.0",
    "cross-env": "^7.0.2",
    "sinon": "^9.0.2"
  },
  "c8": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/@bedrock/meter/**"
    ],
    "exclude": [
      "node_modules/@bedrock/meter/node_modules/**"
    ]
  }
}
