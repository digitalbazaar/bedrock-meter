{
  "name": "bedrock-meter-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test.js test",
    "coverage": "cross-env NODE_ENV=test ESM_OPTIONS='{cache:false}' nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test ESM_OPTIONS='{cache:false}' nyc --reporter=lcovonly npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "base58-universal": "^1.0.0",
    "bedrock": "^4.1.1",
    "bedrock-did-context": "^1.0.0",
    "bedrock-express": "^3.2.0",
    "bedrock-ledger-context": "^15.0.0",
    "bedrock-meter": "file://..",
    "bedrock-mongodb": "^8.0.1",
    "bedrock-package-manager": "^1.0.1",
    "bedrock-security-context": "^3.0.0",
    "bedrock-server": "^2.6.0",
    "bedrock-test": "^5.3.0",
    "bedrock-validation": "^4.4.0",
    "bnid": "^2.0.0",
    "cross-env": "^7.0.2",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-meter/**"
